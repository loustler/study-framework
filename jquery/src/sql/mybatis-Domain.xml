<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE configuration
 		 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 		"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<typeAliases>
		<typeAlias type="com.model2.mvc.service.domain.User"
			alias="user" />
		<typeAlias type="com.model2.mvc.service.domain.Product"
			alias="product" />
		<typeAlias type="com.model2.mvc.service.domain.Purchase"
			alias="purchase" />
		<typeAlias type="com.model2.mvc.common.Search" alias="search" />
	</typeAliases>

	<!-- SQL Definition :: file include -->
	<mappers>
		<mapper resource="sql/UserMapper.xml" />
		<mapper resource="sql/ProductMapper.xml" />
		<mapper resource="sql/PurchaseMapper.xml" />
	</mappers>

</configuration>